/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
/* toolbar  */
.tool-bar {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}
.tool-bar-icon {
  width: 25px;
  height: 25px;
  margin: 20rpx;
}
/* icon  */
